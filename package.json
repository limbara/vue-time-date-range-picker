{
  "name": "vue-time-date-range-picker",
  "version": "2.2.0",
  "description": "a vue time date range picker",
  "keywords": [
    "vue",
    "vuejs",
    "range-datetime-picker",
    "date-time-range-picker"
  ],
  "author": "Nico Limbara <nick_dev@yahoo.com>",
  "license": "MIT",
  "private": false,
  "bugs": {
    "url": "https://github.com/limbara/vue-time-date-range-picker/issues"
  },
  "homepage": "https://github.com/limbara/vue-time-date-range-picker#readme",
  "type": "module",
  "files": [
    "dist",
    "!**/*.tsbuildinfo",
    "LICENSE",
    "README.md"
  ],
  "main": "dist/vdprDatePicker.umd.cjs",
  "module": "dist/vdprDatePicker.js",
  "browser": "dist/vdprDatePicker.js",
  "unpkg": "dist/vdprDatePicker.iife.js",
  "types": "dist/src/index.d.ts",
  "style": "dist/style.css",
  "exports": {
    ".": {
      "import": "./dist/vdprDatePicker.js",
      "require": "./dist/vdprDatePicker.umd.cjs",
      "types": "./dist/src/index.d.ts"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css",
      "default": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite -c vite.config.dev.ts",
    "clean": "rimraf ./dist ./dist-zip",
    "prebuild": "npm run clean",
    "build": "vue-tsc -b && vite build",
    "lint": "eslint",
    "test": "jest",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=20.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-typescript": "^7.25.7",
    "@eslint/js": "^9.12.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.13",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "^29.2.6",
    "babel-jest": "^29.7.0",
    "eslint": "^9.12.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-vue": "^9.29.0",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "moment": "2.27.0",
    "postcss-preset-env": "^10.0.7",
    "rimraf": "^6.0.1",
    "sass-embedded": "^1.79.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.8.1",
    "vite": "^5.4.8",
    "vite-plugin-zip-pack": "^1.2.4",
    "vue": "3.2.25",
    "vue-tsc": "^2.1.6"
  },
  "peerDependencies": {
    "moment": "^2.27.0",
    "vue": "^3.2.25"
  },
  "dependencies": {
    "vite-plugin-dts": "^4.3.0"
  }
}
